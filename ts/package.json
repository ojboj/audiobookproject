    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "husky": "^4.3.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "ts-mocha": "^8.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "string-similarity": "^4.0.3",
    "ts-node": "^9.0.0",
