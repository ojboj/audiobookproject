{
  "name": "audiobookproject",
  "version": "0.0.1",
  "description": "Take in an ebook and output an audiobook. Aim to match different, appropriate, voices for dialogue of main speakers.",
  "main": "index.js",
  "repository": "https://github.com/ojboj/audiobookproject.git",
  "author": "Michael Rawcliffe <michael.rawc@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint ./src --ext .ts ",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "compile": "rimraf dist && ttsc",
    "test": "mocha",
    "test:coverage": "nyc npm run test",
    "test:watch": "yarn test -w",
    "build": "yarn lint && yarn compile && yarn test:coverage"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/lodash": "^4.14.144",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.2",
    "@types/sinon": "^7.5.0",
    "@types/sinon-chai": "^3.2.3",
    "@types/string-similarity": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "chai": "^4.2.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^29.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "husky": "^4.2.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.4.0",
    "ts-node": "^8.4.1",
    "ttypescript": "^1.5.7",
    "typescript": "^3.8.3",
    "typescript-transform-paths": "^1.1.11"
  },
  "dependencies": {
    "lodash": "^4.17.15",
